[
  {
    "name": "LLVM Apple",
    "type": "cfamily",
    "platforms": [
      "macos",
      "ios"
    ],
    "progs": {
      "cc": "clang",
      "cxx": "clang++",
      "objc": "clang",
      "objcxx": "clang++",
      "ld_so": "ld",
      "ld_exe": "ld",
      "ar": "ar"
    },
